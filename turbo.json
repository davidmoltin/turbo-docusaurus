{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^build"],
      "outputs": ["apps/*/build", "root/composer", "root/core"]
    },
    "lint": {},
    "dev": {
      "persistent": true,
      "cache": true
    }
  }
}
